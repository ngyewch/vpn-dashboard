{
  "name": "vpn-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "doDev": "vite",
    "doBuild": "cross-env NODE_ENV=production vite build",
    "doPreview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "watch:check": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "run-p watch:* doDev",
    "build": "run-s check doBuild",
    "preview": "run-s build doPreview"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@tsconfig/svelte": "5.0.4",
    "cross-env": "7.0.3",
    "npm-run-all": "4.1.5",
    "sass": "1.77.7",
    "svelte-check": "3.8.4",
    "svelte-preprocess": "6.0.2",
    "typescript": "5.5.3",
    "vite": "5.3.3"
  },
  "dependencies": {
    "@smui/button": "7.0.0",
    "@smui/data-table": "7.0.0",
    "@smui/paper": "7.0.0",
    "@smui/select": "7.0.0",
    "ky": "1.4.0",
    "moment": "2.30.1",
    "pretty-bytes": "6.1.1",
    "svelte": "4.2.18",
    "svelte-material-ui": "7.0.0",
    "svelte-toasts": "1.1.2"
  }
}
